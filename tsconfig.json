{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
  "baseUrl": ".",
  "paths": {
    "@/*": ["src/*"],
    "@components/*": ["src/components/*"],
    "@layouts/*": ["src/layouts/*"],
    "@lib/*": ["../lib/*"],
    "@styles/*": ["src/styles/*"],
    "@templates/*": ["src/components/templates/*"],
    "@assets/*": ["src/assets/*"],
    "@database/*": ["../database/*"],
    "@api/*": ["src/pages/api/*"],
    "@images/*": ["public/images/*"]
  }
}
}